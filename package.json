{
  "name": "vuelize",
  "version": "0.0.67",
  "private": false,
  "description": "A Vue component Library, with the most common Material UI styled components",
  "author": "Daniel Terletzkiy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DanielTerletzkiy/vuelize"
  },
  "homepage": "https://danielterletzkiy.github.io/vuelize",
  "main": "dist/vuelize.umd.js",
  "module": "dist/vuelize.esm.js",
  "unpkg": "dist/vuelize.min.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "build": "npm run build:umd && npm run build:es && npm run build:unpkg",
    "build:publish": "npm run build && npm publish",
    "build:umd": "rollup --config rollup.config.js --format umd --file dist/vuelize.umd.js",
    "build:es": "rollup --config rollup.config.js --format es --file dist/vuelize.esm.js",
    "build:unpkg": "rollup --config rollup.config.js --format iife --file dist/vuelize.min.js",
    "build:docs": "vue-cli-service build --dest gh-pages && git add gh-pages && git commit -m'adding gh-pages to subtree' && git subtree push --prefix gh-pages origin gh-pages"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "jss": "^10.8.2",
    "jss-plugin-nested": "^10.8.2",
    "vue": "^2.6.11",
    "vue-material-design-ripple": "^1.0.0",
    "vue-router": "^3.5.3",
    "vue-unicons": "^3.3.0",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "jss-preset-default": "^10.8.2",
    "node-sass": "^6.0.1",
    "rollup": "^1.17.0",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-vue": "^5.0.0",
    "sass": "^1.43.5",
    "sass-loader": "^10",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
