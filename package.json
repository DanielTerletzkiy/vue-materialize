{
  "name": "vuelize",
  "version": "0.0.79",
  "private": false,
  "description": "A Vue component Library, with the most common Material UI styled components",
  "author": "Daniel Terletzkiy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DanielTerletzkiy/vuelize"
  },
  "homepage": "https://danielterletzkiy.github.io/vuelize",
  "main": "dist/vuelize.umd.js",
  "module": "dist/vuelize.esm.js",
  "unpkg": "dist/vuelize.min.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "vue",
    "vue2",
    "components",
    "material-like"
  ],
  "scripts": {
    "serve": "vue-cli-service serve DOCS/main.js",
    "lint": "vue-cli-service lint",
    "build": "npm run build:umd && npm run build:es && npm run build:unpkg",
    "build:rollup": "cross-env NODE_ENV=production rollup --config ./rollup.config.js",
    "build:publish": "npm run build:rollup && np",
    "build:docs": "vue-cli-service build --dest gh-pages DOCS/main.js && git add gh-pages && git commit -m'adding gh-pages to subtree' && git subtree push --prefix gh-pages origin gh-pages && git push"
  },
  "dependencies": {
    "@rollup/plugin-alias": "^3.1.8",
    "@rollup/plugin-babel": "^5.3.0",
    "core-js": "^3.6.5",
    "jss": "^10.8.2",
    "jss-plugin-nested": "^10.8.2",
    "minimist": "^1.2.5",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "v-runtime-template": "^1.10.0",
    "vue": "^2.6.11",
    "vue-highlightjs": "^1.3.3",
    "vue-material-design-ripple": "^1.0.0",
    "vue-router": "^3.5.3",
    "vue-unicons": "^3.3.0",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@rollup/plugin-replace": "^3.0.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "jss-preset-default": "^10.8.2",
    "node-sass": "^6.0.1",
    "postcss": "^8.4.5",
    "raw-loader": "^4.0.2",
    "rollup": "^2.42.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-vue": "^5.0.0",
    "sass": "^1.43.5",
    "sass-loader": "^10",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  }
}
